<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.planetsystems.tela">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activities.ClockIn_with_StaffId"
            android:theme="@android:style/Theme.Dialog"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activities.fingerprint.FingerPrintActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".activities.clockInAndOutActivity.ClockInAndOutActivity" />
        <activity
            android:name=".activities.enrollActivity.EnrollmentActivity"
            android:parentActivityName=".activities.clockInAndOutActivity.ClockInAndOutActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name=".activities.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>